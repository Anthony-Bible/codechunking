# Example environment variables for CodeChunking
# Copy this file to .env and fill in your values

# Database Configuration
CODECHUNK_DATABASE_USER=dev
CODECHUNK_DATABASE_PASSWORD=dev
CODECHUNK_DATABASE_HOST=localhost
CODECHUNK_DATABASE_PORT=5432
CODECHUNK_DATABASE_NAME=codechunking

# NATS Configuration
CODECHUNK_NATS_URL=nats://localhost:4222

# Gemini API Configuration
CODECHUNK_GEMINI_API_KEY=your-gemini-api-key-here
CODECHUNK_GEMINI_MODEL=gemini-embedding-001

# API Configuration
CODECHUNK_API_PORT=8080
CODECHUNK_API_HOST=0.0.0.0

# Worker Configuration
CODECHUNK_WORKER_CONCURRENCY=5
CODECHUNK_WORKER_QUEUE_GROUP=workers

# Logging Configuration
CODECHUNK_LOG_LEVEL=info
CODECHUNK_LOG_FORMAT=json